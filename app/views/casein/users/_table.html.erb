<!-- Scaffolding generated by Casein v5.0.0.0 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Name", :name %></th>
			<th><%= casein_sort_link "Email", :email %></th>
			<th><%= casein_sort_link "Nickname", :nickname %></th>
			<th><%= casein_sort_link "Provider", :provider %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% users.each do |users| %>
		<tr>
			<td><%= casein_table_cell_link users.name, casein_users_path(users) %></td>
			<td><%= casein_table_cell_link users.email, casein_users_path(users) %></td>
			<td><%= casein_table_cell_link users.nickname, casein_users_path(users) %></td>
			<td><%= casein_table_cell_link users.provider, casein_users_path(users) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_users_path(users), :method => :delete, :data => { :confirm => "Are you sure you want to delete this users?" }) %>
			</td>
		</tr>
	<% end %>
</table>